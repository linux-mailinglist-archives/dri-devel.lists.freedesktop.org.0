 	u32 bus_flags, bus_fmt;
 
 	next_bridge = drm_bridge_get_next_bridge(bridge);
-	if (next_bridge)
+	if (next_bridge) {
 		next_bridge_state = drm_atomic_get_new_bridge_state(crtc_state->state,
