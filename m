>  		regmap_write(dsi->grf_regmap, dsi->cdata->lanecfg1_grf_reg,
>  					      dsi->cdata->lanecfg1);
> @@ -711,6 +706,13 @@ static void dw_mipi_dsi_rockchip_config(struct dw_mipi_dsi_rockchip *dsi,
>  					      dsi->cdata->enable);
