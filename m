> +	ctx->backlight = devm_backlight_device_register(dev, dev_name(dev), dev,
> +							dsi, &s6e8aa5x01_bl_ops,
> +							NULL);
> +	if (IS_ERR(ctx->backlight))
> +		return dev_err_probe(dev, PTR_ERR(ctx->backlight),
> +				     "Failed to register backlight device\n");
