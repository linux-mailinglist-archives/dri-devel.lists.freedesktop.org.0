+	if (ctx.accum_err)
+		return ctx.accum_err;
+
+	msleep(100);
+
+	mipi_dsi_dcs_write_seq_multi(&ctx, 0x11);
+	if (ctx.accum_err)
+		return ctx.accum_err;
+
+	msleep(200);
+
+	mipi_dsi_dcs_write_seq_multi(&ctx, 0x29);
+	if (ctx.accum_err)
+		return ctx.accum_err;
+
+	msleep(100);
+
+	return 0;
 };
 
